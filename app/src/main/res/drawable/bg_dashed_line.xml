<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item>
        <shape>
            <solid android:color="@color/white" />
            <stroke
                android:dashGap="@dimen/size_5"
                android:dashWidth="@dimen/size_8"
                android:width="1dp"
                android:color="#ACADC1" />
            <padding
                android:bottom="5dp"
                android:left="5dp"
                android:right="5dp"
                android:top="5dp" />

            <corners android:radius="@dimen/size_12"/>
        </shape>
    </item>
</layer-list>